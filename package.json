{
  "name": "why-am-i-here-extension",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.config.ts",
    "build": "npm run build:options && npm run build:content && npm run build:background",
    "build:options": "vite build --config vite.config.ts",
    "build:content": "vite build --config vite.content.config.ts",
    "build:background": "vite build --config vite.background.config.ts",
    "preview": "vite preview --config vite.config.ts"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "wink-lemmatizer": "^3.0.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "playwright": "^1.58.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
